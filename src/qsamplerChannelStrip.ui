                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Total stream usage (%)</string>
                    </property>
                </widget>
                <widget class="QLabel">
                    <property name="name">
                        <cstring>VoiceCountTextLabel</cstring>
                    </property>
                    <property name="minimumSize">
                        <size>
