                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>4</number>
                </property>
                <property name="spacing">
                    <number>4</number>
                </property>
                <widget class="QToolButton">
                    <property name="name">
                        <cstring>ChannelToolButton</cstring>
                    </property>
                    <property name="text">
