<author>rncbc aka Rui Nuno Capela</author>
<class>qsamplerChannelForm</class>
<widget class="QWidget">
    <property name="name">
        <cstring>qsamplerChannelForm</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>752</width>
            <height>102</height>
        </rect>
    </property>
    <property name="sizePolicy">
        <sizepolicy>
            <hsizetype>7</hsizetype>
            <vsizetype>1</vsizetype>
            <horstretch>0</horstretch>
            <verstretch>0</verstretch>
        </sizepolicy>
    </property>
    <property name="caption">
        <string>Channel</string>
    </property>
    <property name="focusPolicy">
        <enum>StrongFocus</enum>
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <widget class="QLabel" row="0" column="0">
            <property name="name">
                <cstring>EngineTextLabel</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>1</hsizetype>
                    <vsizetype>5</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="text">
                <string>&amp;Engine:</string>
            </property>
            <property name="alignment">
                <set>AlignVCenter</set>
            </property>
            <property name="buddy" stdset="0">
                <cstring>EngineComboBox</cstring>
            </property>
        </widget>
        <widget class="QGroupBox" row="1" column="0" rowspan="2" colspan="4">
            <property name="name">
                <cstring>MidiGroupBox</cstring>
            </property>
            <property name="title">
                <string>&amp;MIDI/Input</string>
            </property>
            <hbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>8</number>
                </property>
                <property name="spacing">
                    <number>8</number>
                </property>
                <widget class="QLabel">
                    <property name="name">
                        <cstring>MidiTypeTextLabel</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Type:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>MidiTypeComboBox</cstring>
                    </property>
                </widget>
                <widget class="QComboBox">
                    <item>
                        <property name="text">
                            <string>ALSA</string>
                        </property>
                    </item>
                    <property name="name">
                        <cstring>MidiTypeComboBox</cstring>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>MIDI input driver type</string>
                    </property>
                </widget>
                <widget class="QLabel">
                    <property name="name">
                        <cstring>MidiPortTextLabel</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Port:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>MidiPortLineEdit</cstring>
                    </property>
                </widget>
                <widget class="QLineEdit">
                    <property name="name">
                        <cstring>MidiPortLineEdit</cstring>
                    </property>
                    <property name="sizePolicy">
                        <sizepolicy>
                            <hsizetype>1</hsizetype>
                            <vsizetype>0</vsizetype>
                            <horstretch>0</horstretch>
                            <verstretch>0</verstretch>
                        </sizepolicy>
                    </property>
                    <property name="minimumSize">
                        <size>
                            <width>30</width>
                            <height>0</height>
                        </size>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>MIDI input port</string>
                    </property>
                </widget>
                <widget class="QLabel">
                    <property name="name">
                        <cstring>MidiChannelTextLabel</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Channel:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>MidiChannelSpinBox</cstring>
                    </property>
                </widget>
                <widget class="QSpinBox">
                    <property name="name">
                        <cstring>MidiChannelSpinBox</cstring>
                    </property>
                    <property name="maxValue">
                        <number>16</number>
                    </property>
                    <property name="minValue">
                        <number>1</number>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>MIDI input channel</string>
                    </property>
                </widget>
            </hbox>
        </widget>
        <widget class="QGroupBox" row="1" column="4" rowspan="2" colspan="2">
            <property name="name">
                <cstring>AudioGroupBox</cstring>
            </property>
            <property name="title">
                <string>&amp;Audio/Output</string>
            </property>
            <hbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>8</number>
                </property>
                <property name="spacing">
                    <number>8</number>
                </property>
                <widget class="QLabel">
                    <property name="name">
                        <cstring>AudioTypeTextLabel</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Type:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>AudioTypeComboBox</cstring>
                    </property>
                </widget>
                <widget class="QComboBox">
                    <item>
                        <property name="text">
                            <string>ALSA</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>JACK</string>
                        </property>
                    </item>
                    <property name="name">
                        <cstring>AudioTypeComboBox</cstring>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Audio output driver type</string>
                    </property>
                </widget>
                <widget class="QLabel">
                    <property name="name">
                        <cstring>AudioChannelTextLabel</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Channel:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>AudioChannelSpinBox</cstring>
                    </property>
                </widget>
                <widget class="QSpinBox">
                    <property name="name">
                        <cstring>AudioChannelSpinBox</cstring>
                    </property>
                    <property name="maxValue">
                        <number>16</number>
                    </property>
                    <property name="minValue">
                        <number>1</number>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Audio output channel </string>
                    </property>
                </widget>
            </hbox>
        </widget>
        <widget class="QLabel" row="0" column="2">
            <property name="name">
                <cstring>InstrumentTextLabel</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>1</hsizetype>
                    <vsizetype>5</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="text">
                <string>&amp;Instrument:</string>
            </property>
            <property name="alignment">
                <set>AlignVCenter|AlignRight</set>
            </property>
            <property name="buddy" stdset="0">
                <cstring>InstrumentLineEdit</cstring>
            </property>
        </widget>
        <widget class="QLineEdit" row="0" column="3" rowspan="1" colspan="2">
            <property name="name">
                <cstring>InstrumentLineEdit</cstring>
            </property>
            <property name="readOnly">
                <bool>true</bool>
            </property>
            <property name="toolTip" stdset="0">
                <string>Instrument filename</string>
            </property>
        </widget>
        <widget class="QPushButton" row="0" column="5">
            <property name="name">
                <cstring>InstrumentPushButton</cstring>
            </property>
            <property name="minimumSize">
                <size>
                    <width>20</width>
                    <height>22</height>
                </size>
            </property>
            <property name="maximumSize">
                <size>
                    <width>22</width>
                    <height>24</height>
                </size>
            </property>
            <property name="text">
                <string>...</string>
            </property>
        </widget>
        <widget class="QGroupBox" row="0" column="6" rowspan="3" colspan="1">
            <property name="name">
                <cstring>VolumeGroupBox</cstring>
            </property>
            <property name="title">
                <string>&amp;Volume</string>
            </property>
            <property name="alignment">
                <set>AlignAuto</set>
            </property>
            <vbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <property name="margin">
                    <number>4</number>
                </property>
                <property name="spacing">
                    <number>4</number>
                </property>
                <widget class="QDial">
                    <property name="name">
                        <cstring>VolumeDial</cstring>
                    </property>
                    <property name="sizePolicy">
                        <sizepolicy>
                            <hsizetype>1</hsizetype>
                            <vsizetype>1</vsizetype>
                            <horstretch>0</horstretch>
                            <verstretch>0</verstretch>
                        </sizepolicy>
                    </property>
                    <property name="maximumSize">
                        <size>
                            <width>40</width>
                            <height>40</height>
                        </size>
                    </property>
                    <property name="notchTarget">
                        <number>10</number>
                    </property>
                    <property name="notchesVisible">
                        <bool>true</bool>
                    </property>
                    <property name="maxValue">
                        <number>100</number>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Channel volume</string>
                    </property>
                </widget>
                <widget class="QSpinBox">
                    <property name="name">
                        <cstring>VolumeSpinBox</cstring>
                    </property>
                    <property name="maximumSize">
                        <size>
                            <width>40</width>
                            <height>32767</height>
                        </size>
                    </property>
                    <property name="maxValue">
                        <number>100</number>
                    </property>
                    <property name="toolTip" stdset="0">
                        <string>Channel volume</string>
                    </property>
                </widget>
            </vbox>
        </widget>
        <widget class="QProgressBar" row="2" column="7" rowspan="1" colspan="2">
            <property name="name">
                <cstring>StreamUsageProgressBar</cstring>
            </property>
            <property name="indicatorFollowsStyle">
                <bool>false</bool>
            </property>
            <property name="toolTip" stdset="0">
                <string>Total stream usage (%)</string>
            </property>
        </widget>
        <widget class="QLabel" row="0" column="7">
            <property name="name">
                <cstring>VoiceCountTextLabel</cstring>
            </property>
            <property name="text">
                <string>Voice count:</string>
            </property>
        </widget>
        <widget class="QLabel" row="0" column="8">
            <property name="name">
                <cstring>VoiceCountTextValue</cstring>
            </property>
            <property name="minimumSize">
                <size>
                    <width>30</width>
                    <height>0</height>
                </size>
            </property>
            <property name="maximumSize">
                <size>
                    <width>60</width>
                    <height>32767</height>
                </size>
            </property>
            <property name="paletteForegroundColor">
                <color>
                    <red>0</red>
                    <green>255</green>
                    <blue>0</blue>
                </color>
            </property>
            <property name="paletteBackgroundColor">
                <color>
                    <red>0</red>
                    <green>0</green>
                    <blue>0</blue>
                </color>
            </property>
            <property name="frameShape">
                <enum>Panel</enum>
            </property>
            <property name="frameShadow">
                <enum>Sunken</enum>
            </property>
            <property name="text">
                <string>0</string>
            </property>
            <property name="alignment">
                <set>AlignCenter</set>
            </property>
        </widget>
        <widget class="QLabel" row="1" column="7" rowspan="1" colspan="2">
            <property name="name">
                <cstring>StreamUsageTextLabel</cstring>
            </property>
            <property name="text">
                <string>Stream usage:</string>
            </property>
            <property name="alignment">
                <set>AlignBottom</set>
            </property>
        </widget>
        <widget class="QComboBox" row="0" column="1">
            <property name="name">
                <cstring>EngineComboBox</cstring>
            </property>
            <property name="sizePolicy">
                <sizepolicy>
                    <hsizetype>7</hsizetype>
                    <vsizetype>0</vsizetype>
                    <horstretch>0</horstretch>
                    <verstretch>0</verstretch>
                </sizepolicy>
            </property>
            <property name="toolTip" stdset="0">
                <string>Engine name</string>
            </property>
        </widget>
    </grid>
</widget>
<tabstops>
    <tabstop>EngineComboBox</tabstop>
    <tabstop>InstrumentLineEdit</tabstop>
    <tabstop>InstrumentPushButton</tabstop>
    <tabstop>MidiTypeComboBox</tabstop>
    <tabstop>MidiPortLineEdit</tabstop>
    <tabstop>MidiChannelSpinBox</tabstop>
    <tabstop>AudioTypeComboBox</tabstop>
    <tabstop>AudioChannelSpinBox</tabstop>
    <tabstop>VolumeDial</tabstop>
    <tabstop>VolumeSpinBox</tabstop>
</tabstops>
<functions>
    <function access="private" specifier="non virtual">init()</function>
    <function access="private" specifier="non virtual">destroy()</function>
</functions>
<layoutdefaults spacing="8" margin="8"/>
</UI>
